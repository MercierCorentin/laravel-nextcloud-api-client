        }
    },
    "autoload-dev": {
        "psr-4": {
            "MercierCorentin\\Nextcloud\\Test\\":"tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.0"
    }
}
