{
    "name": "mercier-corentin/nextcloud",
    "description": "Nextcloud api client for laravel",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "nextcloud"],
    "authors": [
        {
            "name": "MercierCorentin",
            "email": "corentin@cmercier.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2.0"
    },
    "autoload": {
        "psr-4": {
